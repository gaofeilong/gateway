[GatewayArchive]

#归档程序运行间隔时间
intervalTime=1

#归档到目前运行了多少天
cronTime=1

#开始时间
startTime=00:00

#是否校验
isCheck=1

#归档几天前的数据
modifyTime=1

#归档程序运行时长
runTime=1

#归档结束时间
endTime=00:00

#更新间隔条数，即每隔多少条数据更新一次数据库
updateInterval=100

#归档失败记录所在目录路径
arvErrPath=/var/spool/scigw/ErrArvTask/

#归档详细任务目录路径
arvTaskPath=/var/spool/scigw/ArvTask/

#归档完成任务路径
arvFinishedPath=/var/spool/scigw/FinishedTask/

#归档源-目的路径对(分区或者目录)
arvDevPath=/etc/scigw/archive.cnf

#归档过滤条件文件路径
filter=/etc/scigw/filter.cnf

#归档启动标识文件
runStatus=/etc/scigw/run.sig

#获取哪个网卡的
networkId=eth0

#挂载后路径
mountpoint=/mnt/b

#移动日志文件中记录条数
mvMaxRecord=30

#归档最大线程数
arvMaxThreadCnt=4

#线程是否按分区来开启
classifyDev=1

#PID文件路径
pidPath=/etc/scigw/pid

#指定磁盘剩余空间, 单位G
arvLeftSpace=50

#指定节点类型, 0:通用节点 1:神通节点, 2:南京节点
nodeType=0

#指定神通数据库配置文件路径
stConfPath=/etc/scigw/st.conf

[Filter]

#0:不设置；1:指定；2:过滤
prefixType=0

prefix=

#0:不设置；1:指定；2:过滤
postfixType=0

postfix=

#0:不设置；1:指定；2:过滤
dirType=0

dir=

#0:不设置；1:指定；2:过滤
sizeType=0

#0:不指定边界值；other: 指定边界值
minSize=

maxSize=

